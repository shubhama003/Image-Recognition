    "import matplotlib.pyplot as plt\n",
    "import cv2\n",
    "image = cv2.imread(\"5_test.jpg\")\n",
    "im = cv2.cvtColor(image,cv2.COLOR_RGB2GRAY)\n",
    "im = cv2.resize(image, (28, 28))\n",
    "cv2.imwrite( \"greyscale.jpg\", im )\n",
    "\n",
    "img=cv2.imread(\"greyscale.jpg\")\n",
    "plt.imshow(img)\n",
    "plt.show()\n"
   ]
